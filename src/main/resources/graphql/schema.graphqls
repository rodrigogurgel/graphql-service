scalar DateTime

enum PetType {
    CAT
    DOG
    BIRD
}

input PetInput {
    name: String
    type: PetType
}

type Pet {
    id: ID
    name: String
    type: PetType
}

input UserInput {
    name: String
}

type User {
    id: ID!
    name: String
    pets: [Pet!]
}

type Mutation {
    createUser(userInput: UserInput!): User
    createPet(userId: ID!, petInput: PetInput!): Pet
}

type Query {
    users: [User!]
    findUserById(id: ID!): User!
}